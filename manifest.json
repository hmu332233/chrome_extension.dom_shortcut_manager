{
  "manifest_version": 2,
  "name": "dom_shorcut_manager",
  "version": "0.0.1",
  "description": "",
  "browser_action": {
    "default_title": "dom shortcut manager",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["build/contentScripts.js"]
    }
  ],
  "permissions": ["activeTab"]
}